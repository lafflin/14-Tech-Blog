{{#if posts}}

<ul id= "postList" class="post-list">
    {{#each posts as |post|}}
    <li>
        {{> post-format}}
    </li>
    {{/each}}
</ul>
{{else}}
<h1>You have not posted yet</h1>
{{/if}}

<button class="bg-success" >
<a  class="nav-link" href="/newPost">
<i class="fas fa-plus-circle"></i>
</a>
</button>